{
  "schemaVersion": 1,
  "id": "cwcrystaladdon",
  "version": "${version}",
  "name": "Addon Template",
  "description": "CwCrystal",
  "authors": [
    "Dinolog"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.dino.cwaddon.CwAddon"
    ]
  },
  "mixins": [
    "cwcrystal.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
